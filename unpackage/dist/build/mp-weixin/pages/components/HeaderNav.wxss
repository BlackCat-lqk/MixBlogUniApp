.container{height:100%;background-color:#f4f2ec;display:flex;flex-direction:column}.navbar{display:flex;flex-direction:row;justify-content:space-between;align-items:center;height:44px;padding:0 10px;background-color:#fff;border-bottom:1px solid #e0e0e0;position:relative;z-index:999}.nav-left{display:flex;align-items:center}.logo{width:30px;height:30px}.nav-center{flex:1;display:flex;flex-direction:row;justify-content:space-around;align-items:center;padding:0 16px}.nav-item{font-size:14px;color:#333;padding:5px 10px}.nav-item:first-child{color:#007aff;font-weight:700;border-bottom:2px solid #007AFF}.nav-right{display:flex;justify-content:flex-end;align-items:center;position:relative}.more-icon{font-family:customicons!important;font-size:20px;color:#333;cursor:pointer}.popup-menu{background-color:#fff;border-radius:0 0 5px 5px;box-shadow:0 2px 10px rgba(0,0,0,.1);width:120px;margin-left:auto;margin-top:44px;margin-right:10px}.menu-item{padding:15px;font-size:14px;color:#333;text-align:center;border-bottom:1px solid #f0f0f0}.menu-item:last-child{border-bottom:none}.menu-item:active{background-color:#f0f0f0}
